if game:GetService("ReplicatedStorage").GameData.Floor.Value == "Fools" then
local RemotesFolder = game:GetService("ReplicatedStorage").EntityInfo
end
if not game:GetService("ReplicatedStorage").GameData.Floor.Value == "Fools" and not game:GetService("ReplicatedStorage").Bricks then
local RemotesFolder = game:GetService("ReplicatedStorage").RemotesFolder
end
if game:GetService("ReplicatedStorage").Bricks then
local RemotesFolder = game:GetService("ReplicatedStorage").Bricks
end
